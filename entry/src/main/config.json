{
  "app": {
    "bundleName": "com.ohos.photos",
    "vendor": "ohos",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    }
  },
  "deviceConfig": {},
  "module": {
    "package": "com.ohos.photos",
    "name": ".MyApplication",
    "mainAbility": ".MainAbility",
    "deviceType": [
      "phone"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry",
      "installationFree": false
    },
    "abilities": [
      {
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ],
        "orientation": "unspecified",
        "visible": true,
        "srcPath": "MainAbility",
        "name": ".MainAbility",
        "srcLanguage": "ets",
        "icon": "$media:ohos_gallery",
        "description": "$string:description_mainability",
        "formsEnabled": false,
        "label": "$string:app_name",
        "type": "page",
        "launchType": "singleton"
      }
    ],
    "js": [
      {
        "mode": {
          "syntax": "ets",
          "type": "pageAbility"
        },
        "pages": [
          "product/phone/view/index",
          "product/pad/view/index",
          "feature/browser/view/photoGrid/PhotoGridPage",
          "feature/browser/view/photo/PhotoBrowser",
          "feature/browser/view/photo/VideoBrowser",
          "feature/browser/view/photo/SelectPhotoBrowser",
          "feature/thirdSelect/view/ThirdSelectPhotoBrowser",
          "feature/thirdSelect/view/ThirdSelectAlbumSetPage",
          "feature/thirdSelect/view/ThirdSelectPhotoGridPage",
          "feature/browser/view/album/DistributedAlbumSetPage",
          "common/view/mediaOperation/MediaOperationPage",
          "feature/browser/view/album/NewAlbumPage",
          "feature/browser/view/album/AlbumSelect",
          "feature/editor/view/EditMain"
        ],
        "name": ".MainAbility",
        "window": {
          "designWidth": 720,
          "autoDesignWidth": false
        }
      }
    ],
    "reqPermissions": [
      {
        "name": "ohos.permission.READ_MEDIA",
        "reason": "$string:storage_permission_description"
      },
      {
        "name": "ohos.permission.WRITE_MEDIA",
        "reason": "$string:storage_permission_description"
      },
      {
        "name": "ohos.permission.WRITE_EXTERNAL_MEDIA_MEMORY",
        "reason": "$string:storage_permission_description"
      },
      {
        "name": "ohos.permission.MEDIA_LOCATION",
        "reason": "$string:storage_permission_description"
      },
      {
        "name": "ohos.permission.REQUIRE_FORM",
        "reason": "request permission"
      },
      {
        "name": "ohos.permission.GET_BUNDLE_INFO_PRIVILEGED",
        "reason": "request permission"
      }
    ]
  }
}